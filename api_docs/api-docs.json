{
    "openapi": "3.0.0",
    "info": {
        "title": "Binar API Documentation",
        "description": "Car Management API",
        "version": "1.0.0"
    },
    "servers": [
        {
            "url": "http://localhost:3000",
            "description": "localhost"
        }
    ],
    "tags": [
        {
            "name": "Users",
            "description": "Get Endpoint to Users table"
        },
        {
            "name": "Cars",
            "description": "Get Endpoint to Cars table"
        }
    ],
    "components": {
        "schemas": {
            "User": {
                "type": "object",
                "properties": {
                    "id": {
                        "type": "integer"
                    },
                    "role": {
                        "type": "string"
                    },
                    "email": {
                        "type": "string"
                    },
                    "password": {
                        "type": "string"
                    }
                },
                "required": [
                    "id",
                    "role",
                    "email",
                    "password"
                ],
                "example": {
                    "email": "user@gmail.com",
                    "password": "mypassuser"
                }
            },
            "Superadmin": {
                "type": "object",
                "properties": {
                    "id": {
                        "type": "integer"
                    },
                    "role": {
                        "type": "string"
                    },
                    "email": {
                        "type": "string"
                    },
                    "password": {
                        "type": "string"
                    }
                },
                "required": [
                    "id",
                    "role",
                    "email",
                    "password"
                ],
                "example": {
                    "email": "superadmin@gmail.com",
                    "password": "supersecret"
                }
            },
            "Admin": {
                "type": "object",
                "properties": {
                    "id": {
                        "type": "integer"
                    },
                    "role": {
                        "type": "string"
                    },
                    "email": {
                        "type": "string"
                    },
                    "password": {
                        "type": "string"
                    }
                },
                "required": [
                    "id",
                    "role",
                    "email",
                    "password"
                ],
                "example": {
                    "email": "admin@gmail.com",
                    "password": "adminadmin"
                }
            },
            "Car": {
                "type": "object",
                "properties": {
                    "id": {
                        "type": "integer"
                    },
                    "name": {
                        "type": "string"
                    },
                    "price": {
                        "type": "integer"
                    }
                },
                "required": [
                    "id",
                    "name",
                    "price"
                ],
                "example": {
                    "name": "toyota x pander",
                    "price": 300000000
                }
            }
        },
        "securitySchemes": {
            "bearerAuth": {
                "type": "http",
                "scheme": "bearer",
                "bearerFormat": "JWT",
                "response": {
                    "403": {
                        "description": "Unauthorized"
                    }
                }
            }
        }
    },
    "paths": {
        "/users/register": {
            "post": {
                "tags": [
                    "Users"
                ],
                "summary": "Create a user with role member",
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "$ref": "#/components/schemas/User"
                            }
                        }
                    }
                },
                "responses": {
                    "201": {
                        "description": "Created"
                    },
                    "404": {
                        "description": "Data Not Found"
                    },
                    "500": {
                        "description": "Internal Server Error"
                    }
                }
            }
        },
        "/users/login": {
            "post": {
                "tags": [
                    "Users"
                ],
                "summary": "Get token with email and password",
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "$ref": "#/components/schemas/Superadmin"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "OK"
                    },
                    "404": {
                        "description": "email or password incorrect"
                    },
                    "500": {
                        "description": "Internal Server Error"
                    }
                }
            }
        },
        "/users/add_admin": {
            "post": {
                "tags": [
                    "Users"
                ],
                "summary": "Create a user with role admin",
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "$ref": "#/components/schemas/Admin"
                            }
                        }
                    }
                },
                "security": [
                    {
                        "bearerAuth": []
                    }
                ],
                "responses": {
                    "201": {
                        "description": "Created"
                    },
                    "404": {
                        "description": "Data Not Found"
                    },
                    "403": {
                        "description": "Unauthorized/ Only superadmin can acces"
                    },
                    "401": {
                        "description": "Invalid token"
                    },
                    "500": {
                        "description": "Internal Server Error"
                    }
                }
            }
        },
        "/user/status": {
            "get": {
                "security": [
                    {
                        "bearerAuth": []
                    }
                ],
                "tags": [
                    "Users"
                ],
                "summary": "Check information user by token",
                "responses": {
                    "200": {
                        "description": "OK"
                    },
                    "404": {
                        "description": "Data Not Found"
                    },
                    "403": {
                        "description": "Unauthorized"
                    },
                    "401": {
                        "description": "Invalid token"
                    },
                    "500": {
                        "description": "Internal Server Error"
                    }
                }
            }
        },
        "/cars": {
            "get": {
                "security": [
                    {
                        "bearerAuth": []
                    }
                ],
                "tags": [
                    "Cars"
                ],
                "summary": "Get all data cars",
                "responses": {
                    "200": {
                        "description": "OK"
                    },
                    "404": {
                        "description": "Data Not Found"
                    },
                    "403": {
                        "description": "Unauthorized"
                    },
                    "401": {
                        "description": "Invalid token"
                    },
                    "500": {
                        "description": "Internal Server Error"
                    }
                }
            }
        },
        "/car": {
            "post": {
                "security": [
                    {
                        "bearerAuth": []
                    }
                ],
                "tags": [
                    "Cars"
                ],
                "summary": "Create a new car",
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "$ref": "#/components/schemas/Car"
                            }
                        }
                    }
                },
                "responses": {
                    "201": {
                        "description": "Created"
                    },
                    "404": {
                        "description": "Data Not Found"
                    },
                    "403": {
                        "description": "Unauthorized/ Only admin and superadmin can acces"
                    },
                    "401": {
                        "description": "Invalid token"
                    },
                    "500": {
                        "description": "Internal Server Error"
                    }
                }
            }
        },
        "/car/{id}": {
            
            "get": {
                "security": [
                    {
                        "bearerAuth": []
                    }
                ],
                "parameters": [
                    {
                        "in": "path",
                        "name": "id",
                        "required": true,
                        "schema": {
                            "type": "integer",
                            "minimum": 1
                        },
                        "description": "The car ID"
                    }
                ],
                
                "tags": [
                    "Cars"
                ],
                "summary": "Get data car by id",
                "responses": {
                    "200": {
                        "description": "OK"
                    },
                    "404": {
                        "description": "Data Not Found"
                    },
                    "403": {
                        "description": "Unauthorized/ Only admin and superadmin can acces"
                    },
                    "401": {
                        "description": "Invalid token"
                    },
                    "500": {
                        "description": "Internal Server Error"
                    }
                }
            },
            "put": {
                "security": [
                    {
                        "bearerAuth": []
                    }
                ],
                "parameters": [
                    {
                        "in": "path",
                        "name": "id",
                        "required": true,
                        "schema": {
                            "type": "integer",
                            "minimum": 1
                        },
                        "description": "The car ID"
                    }
                ],
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "$ref": "#/components/schemas/Car"
                            }
                        }
                    }
                },
                
                "tags": [
                    "Cars"
                ],
                "summary": "Update data car by id",
                "responses": {
                    "201": {
                        "description": "Created"
                    },
                    "404": {
                        "description": "Data Not Found"
                    },
                    "403": {
                        "description": "Unauthorized/ Only admin and superadmin can acces"
                    },
                    "401": {
                        "description": "Invalid token"
                    },
                    "500": {
                        "description": "Internal Server Error"
                    }
                }
            },
            "delete": {
                "security": [
                    {
                        "bearerAuth": []
                    }
                ],
                "parameters": [
                    {
                        "in": "path",
                        "name": "id",
                        "required": true,
                        "schema": {
                            "type": "integer",
                            "minimum": 1
                        },
                        "description": "The car ID"
                    }
                ],
                
                "tags": [
                    "Cars"
                ],
                "summary": "Delete data car by id",
                "responses": {
                    "200": {
                        "description": "OK"
                    },
                    "404": {
                        "description": "Data Not Found"
                    },
                    "403": {
                        "description": "Unauthorized/ Only admin and superadmin can acces"
                    },
                    "401": {
                        "description": "Invalid token"
                    },
                    "500": {
                        "description": "Internal Server Error"
                    }
                }
            }
        }
    }
}